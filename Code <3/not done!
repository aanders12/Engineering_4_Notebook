from math import sqrt
def discriminate():
    a = int(input("Enter coefficent a"))
    b = int(input("Enter coefficent b"))
    c = int(input("Enter coefficent c"))
    dis = (b**2) - (4*a*c)
    
    if 
    dis >= 0 :
        print("two roots possible")
    
print("Quadratic Solver")
